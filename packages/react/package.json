{
  "name": "@arsonar/react",
  "version": "0.6.0-alpha.4",
  "description": "A arsonar module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "scripts": {
    "example": "cd ../../examples/react && yarn dev",
    "test": "tape test/*.js",
    "build": "rm -fr dist/* && run-s buildts:* && ./fix-ts-build",
    "buildts:esm": "tsc -p tsconfig.json",
    "buildts:cjs": "tsc -p tsconfig-cjs.json"
  },
  "dependencies": {
    "@arsonar/client": "^0.6.0-alpha.4",
    "@types/react": "^17",
    "@types/react-dom": "^17"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "standard": "^14.3.3",
    "tape": "^5.0"
  },
  "standard": {
    "env": [
      "es6",
      "node",
      "browser"
    ],
    "ignore": [
      "build"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arso-project/sonar.git"
  },
  "keywords": [
    "arso-project"
  ],
  "author": {
    "name": "arso contributors",
    "email": "hi@arso.xyz"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/arso-project/sonar/issues"
  },
  "homepage": "https://github.com/arso-project/sonar#readme",
  "publishConfig": {
    "access": "public"
  }
}

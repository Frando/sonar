{
  "name": "@arsonar/client",
  "version": "0.6.0-alpha.4",
  "description": "Client for sonar",
  "main": "dist/cjs",
  "module": "dist/esm",
  "scripts": {
    "test": "tape dist/cjs/test/*.js",
    "doc": "run-p doc:*",
    "doc:md": "jsdoc2md --files lib/workspace.js lib/collection.js lib/fs.js --param-list-format list --separators --partial doc-gen/scope.hbs --helper doc-gen/helpers.js > api.md",
    "doc:html": "rm -rf apidocs && jsdoc --configure doc-gen/jsdoc.json --verbose .",
    "doc:tsd": "jsdoc --configure doc-gen/jsdoc-tsd.json --verbose -d . -t ../../node_modules/tsd-jsdoc/dist -r .",
    "prepublishOnly": "npm run build",
    "build": "rm -fr dist/* && run-s buildts:* && ./fix-ts-build",
    "buildts:esm": "tsc -p tsconfig.json",
    "buildts:cjs": "tsc -p tsconfig-cjs.json"
  },
  "types": "types.d.ts",
  "author": "arso contributors",
  "license": "GPL-3.0",
  "dependencies": {
    "@arsonar/common": "^0.6.0-alpha.4",
    "@types/debug": "^4.1.7",
    "@types/eventsource": "^1.1.8",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/randombytes": "^2.0.0",
    "@types/streamx": "^2.9.1",
    "base32-encode": "^1.1.1",
    "debug": "^4.1.1",
    "eventsource": "github:Frando/eventsource",
    "is-buffer": "^2.0.5",
    "isomorphic-fetch": "^2.2.1",
    "parse-dat-url": "^3.0.3",
    "pretty-bytes": "^5.6.0",
    "randombytes": "github:arso-project/randombytes",
    "split2": "^3.2.2",
    "streamx": "^2.11.0"
  },
  "devDependencies": {
    "@arsonar/server": "^0.6.0-alpha.4",
    "@hyperswarm/dht": "^3.6.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "esbuild": "^0.12.1",
    "express-oas-generator": "^1.0.25",
    "find-free-ports": "^2.0.3",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^6.0",
    "minami": "^1.2.3",
    "nanobench-utils": "^0.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "tape": "^5.0",
    "tape-plus": "^1.0.0",
    "temporary-directory": "^1.0.2",
    "tsd-jsdoc": "^2.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

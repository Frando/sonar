{
  "name": "@arsonar/client",
  "version": "0.6.0",
  "description": "Client for sonar",
  "main": "dist/cjs",
  "module": "dist/esm",
  "scripts": {
    "test": "tape dist/cjs/test/*.js",
    "doc": "run-p doc:*",
    "doc:html": "typedoc --excludePrivate --out docs --logLevel Verbose index.ts",
    "prepublishOnly": "npm run build",
    "build": "rm -fr dist/* && run-s buildts:* && ./fix-ts-build",
    "buildts:esm": "tsc -p tsconfig.json",
    "buildts:cjs": "tsc -p tsconfig-cjs.json"
  },
  "types": "types.d.ts",
  "author": "arso contributors",
  "license": "GPL-3.0",
  "dependencies": {
    "@arsonar/common": "^0.6.0",
    "@types/debug": "^4.1.7",
    "@types/eventsource": "^1.1.8",
    "@types/randombytes": "^2.0.0",
    "@types/streamx": "^2.9.1",
    "base32-encode": "^1.1.1",
    "debug": "^4.1.1",
    "events": "^3.3.0",
    "eventsource": "github:Frando/eventsource",
    "fetch-undici": "^3.0.1",
    "is-buffer": "^2.0.5",
    "parse-dat-url": "^3.0.3",
    "pretty-bytes": "^5.6.0",
    "randombytes": "github:arso-project/randombytes",
    "split2": "^3.2.2",
    "streamx": "^2.11.0"
  },
  "devDependencies": {
    "@arsonar/server": "^0.6.0",
    "@hyperswarm/dht": "^3.6.5",
    "find-free-ports": "^2.0.3",
    "nanobench-utils": "^0.1.1",
    "npm-run-all": "^4.1.5",
    "tape": "^5.0",
    "tape-plus": "^1.0.0",
    "temporary-directory": "^1.0.2",
    "typedoc": "^0.23.0-beta.1"
  },
  "publishConfig": {
    "access": "public"
  }
}

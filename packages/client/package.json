{
  "name": "@arsonar/client",
  "version": "0.3.0-alpha.7",
  "description": "Client for sonar",
  "main": "index.js",
  "moduleDISABLED": "dist/sonar-client.bundle.esm.js",
  "scripts": {
    "build": "rollup --config",
    "test": "tape test/*.js",
    "doc": "run-p doc:*",
    "doc:md": "jsdoc2md --files lib/client.js lib/collection.js lib/fs.js --param-list-format list --separators --partial doc-gen/scope.hbs --helper doc-gen/helpers.js > api.md",
    "doc:html": "rm -rf apidocs && jsdoc --configure doc-gen/jsdoc.json --verbose . && mv apidocs/@arsonar/client/*/* apidocs && rm -r apidocs/@arso-project",
    "doc:tsd": "jsdoc -t ../node_modules/tsd-jsdoc/dist -r ."
  },
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@arsonar/common": "^0.3.0-alpha.7",
    "base32-encode": "^1.1.1",
    "debug": "^4.1.1",
    "eventsource": "github:Frando/eventsource",
    "is-buffer": "^2.0.5",
    "isomorphic-fetch": "^2.2.1",
    "parse-dat-url": "^3.0.3",
    "randombytes": "^2.1.0",
    "rollup": "^2.34.0",
    "simple-rpc-protocol": "^0.4.0",
    "simple-websocket": "^8.1.1",
    "streamx": "^2.10.0"
  },
  "devDependencies": {
    "@arsonar/server": "^0.3.0-alpha.7",
    "@hyperswarm/dht": "^3.6.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "express-oas-generator": "^1.0.25",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^6.0",
    "minami": "^1.2.3",
    "nanobench-utils": "^0.1.1",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "tape": "^5.0",
    "tape-plus": "^1.0.0",
    "temporary-directory": "^1.0.2",
    "tsd-jsdoc": "^2.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

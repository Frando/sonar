{
  "name": "@arsonar/common",
  "version": "0.6.1",
  "main": "dist/cjs",
  "module": "dist/esm",
  "description": "Common libraries for Sonar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arso-project/sonar.git"
  },
  "scripts": {
    "test": "tape -r source-map-support/register dist/cjs/test/*.js",
    "build": "rm -fr dist/* && run-s build:* && ./fix-ts-build",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/tape": "^4.13.2",
    "source-map-support": "^0.5.21",
    "tape": "^5.0",
    "text-table": "^0.2.0"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.11",
    "@types/object-path": "^0.11.1",
    "chalk": "^4.1.0",
    "crypto-hash": "^1.3.0",
    "inspect-custom-symbol": "^1.1.1",
    "object-path": "^0.11.4",
    "pino": "^7.9.2",
    "pino-pretty": "^8.0.0",
    "pretty-hash": "^1.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}

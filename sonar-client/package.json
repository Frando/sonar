    "simple-rpc-protocol": "^0.4.0",
    "simple-websocket": "^8.1.1",
    "streamx": "^2.6.0"
  },
  "devDependencies": {
    "@arso-project/sonar-server": "^0.3.0-alpha.7",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^6.0",
    "minami": "^1.2.3",
    "nanobench-utils": "^0.1.1",
    "tape": "^5.0",
    "tape-plus": "^1.0.0",
